{
    "env": {
      "es2021": true,
      "node": true
    },
    "extends": ["prettier"],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "plugins": ["@typescript-eslint", "prettier", "eslint-plugin-prettier"],
    "rules": {
      "prettier/prettier": "error"
    }
  }
  